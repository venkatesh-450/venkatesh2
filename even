#include<stdio.h>
   int main()
   {
           int i=1;
           while(i<=100)
           {
                   if(i%2==0)
                   printf("even:%d\n",i);
           i++;
          }
          for(i=1;i<=100;i++)
          {
                  if(i%2!=0)
                  printf("odd:%d\n",i);
          }
          return 0;
  }

